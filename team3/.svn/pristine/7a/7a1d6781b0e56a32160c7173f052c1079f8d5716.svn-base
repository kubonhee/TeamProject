package net.yangchoo.domain;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class ChatMessageVO {
	private String roomNo;		// 방 번호
	private String messageNo;	// 메세지 번호
	private String message;		// 메세지
	private String senderName;		// 보낸이 이름
	private String senderEmail;		// 보낸이 이메일
	private int unReadCount;	// 안 읽은 메세지 수
	private int sessionCount;	// 현재 세션 수
}
