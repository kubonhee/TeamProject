  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.yangchoo.mapper.AuthMapper">

	<insert id="authorization">
		INSERT INTO MEMBER_AUTH (USEREMAIL,
									 USERAUTH)
							VALUES( #{userEmail},
